<style>
  .cq-dialog {
    font-family: adobe-clean, 'Source Sans Pro', -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, sans-serif;
    background: #f7f7f7;
    border: 1px solid #d6d6d6;
    border-radius: 4px;
    box-shadow: 0 4px 16px rgba(0, 0, 0, 0.2);
    overflow: hidden;
    max-width: 768px;
    margin: 0 auto;
  }
  
  .cq-dialog-header {
    background: #007eb5;
    color: white;
    padding: 12px 16px;
    display: flex;
    align-items: center;
    justify-content: space-between;
  }
  
  .cq-dialog-title {
    font-size: 16px;
    font-weight: 400;
    margin: 0;
  }
  
  .cq-dialog-close {
    background: none;
    border: none;
    color: white;
    font-size: 20px;
    cursor: pointer;
    padding: 0;
    width: 24px;
    height: 24px;
    display: flex;
    align-items: center;
    justify-content: center;
  }
  
  .cq-dialog-content {
    background: white;
    padding: 0;
  }
  
  .cq-tabs {
    display: flex;
    background: #f5f5f5;
    border-bottom: 1px solid #d6d6d6;
  }
  
  .cq-tab {
    padding: 12px 20px;
    cursor: pointer;
    font-size: 14px;
    border-bottom: 2px solid transparent;
    color: #666;
  }
  
  .cq-tab.active {
    border-bottom: 2px solid #007eb5;
    color: #007eb5;
  }
  
  .cq-tab-panel {
    display: none;
    padding: 20px;
  }
  
  .cq-tab-panel.active {
    display: block;
  }
  
  .cq-form-row {
    margin-bottom: 16px;
  }
  
  .cq-form-label {
    display: block;
    font-size: 13px;
    font-weight: 500;
    margin-bottom: 6px;
    color: #494949;
  }
  
  .cq-form-field {
    width: 100%;
    padding: 8px 10px;
    border: 1px solid #cccccc;
    border-radius: 2px;
    font-size: 14px;
    background: white;
  }
  
  .cq-textarea {
    width: 100%;
    padding: 8px 10px;
    border: 1px solid #cccccc;
    border-radius: 2px;
    font-size: 14px;
    background: white;
    min-height: 80px;
    resize: vertical;
  }
  
  .cq-multifield {
    border: 1px solid #d6d6d6;
    border-radius: 2px;
    background: #f8f8f8;
    padding: 10px;
  }
  
  .cq-multifield-item {
    border: 1px solid #d6d6d6;
    border-radius: 2px;
    background: white;
    padding: 15px;
    margin-bottom: 10px;
  }
  
  .cq-multifield-header {
    display: flex;
    justify-content: space-between;
    margin-bottom: 12px;
    padding-bottom: 8px;
    border-bottom: 1px solid #e6e6e6;
  }
  
  .cq-multifield-title {
    font-weight: 500;
    color: #494949;
  }
  
  .cq-multifield-actions {
    display: flex;
    gap: 6px;
  }
  
  .cq-multifield-btn {
    background: #f0f0f0;
    border: 1px solid #ccc;
    border-radius: 2px;
    padding: 4px 8px;
    font-size: 12px;
    cursor: pointer;
    color: #494949;
  }
  
  .cq-multifield-btn.move-up { 
    font-size: 14px; 
    padding: 2px 6px; 
  }
  .cq-multifield-btn.move-down { 
    font-size: 14px; 
    padding: 2px 6px; 
  }
  .cq-multifield-btn.remove { 
    background: #da0010; 
    color: white; 
    border-color: #da0010; 
  }
  
  .cq-field-pair {
    display: grid;
    grid-template-columns: 1fr 1fr;
    gap: 16px;
  }
  
  .cq-fileupload {
    border: 1px dashed #cccccc;
    padding: 20px;
    text-align: center;
    background: #fafafa;
    border-radius: 2px;
    cursor: pointer;
  }
  
  .cq-fileupload-label {
    font-size: 13px;
    color: #666;
  }
  
  .cq-dialog-footer {
    display: flex;
    justify-content: flex-end;
    padding: 16px 20px;
    background: #f5f5f5;
    border-top: 1px solid #d6d6d6;
  }
  
  .cq-dialog-btn {
    padding: 8px 20px;
    border-radius: 2px;
    font-size: 14px;
    cursor: pointer;
    margin-left: 10px;
  }
  
  .cq-dialog-btn.cancel {
    background: #f0f0f0;
    border: 1px solid #ccc;
    color: #333;
  }
  
  .cq-dialog-btn.done {
    background: #007eb5;
    border: 1px solid #007eb5;
    color: white;
  }
</style>

